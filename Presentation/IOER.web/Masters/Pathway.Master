            <div id="mainSection" class="clear" style="min-height:450px;" >
                <asp:ContentPlaceHolder ID="BodyContent" runat="server" />
            </div>
		</div>
    </div>
    <div id="pageFooter">
      <cc1:footer ID="footer1" runat="server"></cc1:footer>
